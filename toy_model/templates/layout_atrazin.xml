<?xml version="1.0"?>

<layout version="0.2">

	<!-- DATA FILE -->

	<data src="{input_file_name}">

		<!-- NEWLY CREATED DATA FIELDS -->

		<column>CalcC_atrazin_{id}</column>

		<column>Mstock</column>

		<column>Mavail</column>

		<column>Munavail</column>

		<column>Fapplic</column>

		<column>M_to_apply</column>

		<column>T_sum</column>

		<column>Fapplicgreen</column>

		<column>M_green_to_apply</column>

	</data>



	<!-- MODEL UNITS -->

	Application- green and agri use

	<model type="greenherb_application" id="appl_{id}" >

		<attribute> atrazin </attribute>

		<attribute> {id} </attribute>

		<parameter name="M_totalperA" value="{R_appl:.10f}" /> g [/m2]

		<parameter name="M_greenperA" value="0" /> g [/m2]

	</model>



	Release

	<model type="mixed_use" id="atrazin_{id}" >

		<attribute> atrazin </attribute>

		<attribute> {id} </attribute>

		<parameter name="A_tot" value="{A_tot:.2f}" /> units: m2

		<parameter name="A_appl" value="{A_appl:.2f}" />

		<parameter name="A_green" value="0.0" />

		<parameter name="M_house" domain="atrazin" value="0.0" /> g

	</model>



	<!-- CONNECTIONS -->

	inputs:

	<connection sourcedata="T"  destobj="appl_{id}" destport="T"/>

	<connection sourcedata="P"  destobj="appl_{id}" destport="P"/>



	<connection sourcedata="Q" destobj="atrazin_{id}" destport="Q" />

	<connection sourcedata="T"  destobj="atrazin_{id}" destport="T"/>

	<connection sourcedata="P"  destobj="atrazin_{id}" destport="P"/>



	<connection sourceobj="appl_{id}" sourceport="f_applic" destobj="atrazin_{id}" destport="F_applic" />

	<connection sourceobj="appl_{id}" sourceport="f_appgreen" destobj="atrazin_{id}" destport="F_appgreen" />



	outputs:

	<connection sourceobj="atrazin_{id}" sourceport="C" destdata="CalcC_atrazin_{id}" />

	<connection sourceobj="atrazin_{id}" sourceport="M_stock" destdata="Mstock" />

	<connection sourceobj="atrazin_{id}" sourceport="M_avail" destdata="Mavail" />

	<connection sourceobj="atrazin_{id}" sourceport="M_unavail" destdata="Munavail" />

	<connection sourceobj="atrazin_{id}" sourceport="F_applic" destdata="Fapplic" />

	<connection sourceobj="appl_{id}" sourceport="M_to_apply" destdata="M_to_apply" />

	<connection sourceobj="appl_{id}" sourceport="T_sum" destdata="T_sum" />

	<connection sourceobj="atrazin_{id}" sourceport="F_appgreen" destdata="Fapplicgreen" />

	<connection sourceobj="appl_{id}" sourceport="M_green_to_apply" destdata="M_green_to_apply" />





	<!-- INITIAL_VALUES -->

	<initials>

	</initials>



	<!-- DISTRIBUTIONS -->

	<distribution type="lognormal" mean="14.3688" stdev="10" name="Cback_prior" />

	<distribution type="lognormal" mean="0.00656074" stdev="0.01" name="k_prior" />

	<distribution type="lognormal" mean="619.71" stdev="200" name="Tobj_prior" />



	<parameters>

		<parameter name="k" domain="atrazin" value="0.00359916" distribution="k_prior"/>

		<parameter name="C_back" domain="atrazin" value="11.7932" distribution="Cback_prior" /> ng/L

		<parameter name="Init_avail" domain="atrazin" value="0.200843" max="1.0" min="1E-20"/>

		<parameter name="Transfer_au" domain="atrazin" value="0.0468199" max="1.0" min="1E-20" />

		<parameter name="Transfer_ua" domain="atrazin" value="1.96775e-005" max="1.0" min="1E-20"/>

		<parameter name="T_objective" domain="atrazin" value="14154.192" distribution="Tobj_prior" max="21600" min="4800"/>





		<parameter name="Eta" domain="atrazin" value="6.63669e-005" /> from best calibration

		<parameter name="Beta" domain="atrazin" value="0.0" /> [loss/d.mm]



		<parameter name="sigma_CalcC_Ror_" value="9.52734" />



	</parameters>



	<evaluation method="Normal">

		<compare modelled="CalcC_atrazin_{id}" measured="Emptymeas">

			<settings>

				Box-Cox transformation

				<lambda_1> 0.3 </lambda_1>

				<lambda_2>0</lambda_2>

			</settings>

		</compare>

	</evaluation>



	<!-- SOLVER SETTINGS -->

<solver accuracy="1e-6" />



</layout>

